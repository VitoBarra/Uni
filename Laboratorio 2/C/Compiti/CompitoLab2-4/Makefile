.PHONY: test1
.PHONY: test2
.PHONY: createTest

Comp: Compito4.c
	@gcc $^ -o $@

test1: Compito4.sh createTest
	@echo "____________________________________________________________"
	./$< -d dir -s 14 -f 0.1 -n 0  
	@echo "____________________________________________________________"
	./$< -d dir1 -s 14 -f 0.35 -n 0
	@echo "____________________________________________________________" 
	./$< -d dir2 -s 8 -f 0.35 -n 1
	@echo "____________________________________________________________"
	./$< -d dir3 -s 11 -f 0.2 -n 0

test2: Comp createTest
	@echo "____________________________________________________________"
	./$< -d dir -s 14 -f 0.1 -n 0
	@echo "____________________________________________________________"
	./$< -d dir1 -s 14 -f 0.35 -n 0
	@echo "____________________________________________________________" 
	./$< -d dir2 -s 8 -f 0.35 -n 1
	@echo "____________________________________________________________"
	./$< -d dir3 -s 11 -f 0.2 -n 0


createTest:
	@./createTest.sh

compito: ../VitoBarraCompito4/
	tar -czvf ../$@.tar.gz $^