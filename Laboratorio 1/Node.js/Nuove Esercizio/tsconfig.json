{
    
    "compilerOptions": {
        "module": "commonjs",
        "target": "ES6",
        "moduleResolution": "node",
        "sourceMap": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "removeComments": false,
        "noImplicitAny": false,
        "outDir": "out",
        "resolveJsonModule": true,
        "esModuleInterop": true,
    },
    "exclude": [
        "node_modules",
        ".npm"
    ]
}